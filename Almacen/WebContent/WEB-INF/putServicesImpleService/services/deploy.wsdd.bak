<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from putServicesImpleService WSDL service -->

  <service name="putServicesImple" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://services"/>
      <parameter name="wsdlServiceElement" value="putServicesImpleService"/>
      <parameter name="schemaQualified" value="http://services"/>
      <parameter name="wsdlServicePort" value="putServicesImple"/>
      <parameter name="className" value="services.PutServicesImpleSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="putServicesImple"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="putCompany" qname="operNS:putCompany" xmlns:operNS="http://services" soapAction="" >
        <parameter qname="pns:nameCompany" xmlns:pns="http://services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="putCompany"/>

  </service>
</deployment>
